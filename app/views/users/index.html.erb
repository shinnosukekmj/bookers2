<div class="container">
    <div class="row">
        
            <%=render 'books/list',new_book: @new_book,user: current_user%>

        <div class="col-md-8 offset-md-1">
            <h1>Users</h1>
            <table>
                <tr>
                    <th>image</th>
                    <th>name</th>
                    <th colspan="3"></th>
                </tr>
                
        <% @users.each do |user| %>
            <tr>
                <td><%= image_tag user.get_proflie_image("50x50") %></td>
                <td>
                <%=user.name %>
                </td>
                <td><%=link_to 'Show',user_path(user.id)%></td>
            </tr>
                <% end %>
            </table>
        </div>
    </div>
</div>